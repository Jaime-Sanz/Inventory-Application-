<!DOCTYPE html>
<html lang="en">
<%- include('partials/header') %> 
<body class="h-screen">
    <div class="flex h-full">
        <%- include('partials/sidebar') %>
        <div class="flex-1 flex flex-wrap overflow-auto align-middle">
            <% items.forEach(item => { %>
                <div class="item w-16 m-2" data-name="<%= item.name %>">
                    <img src="<%= item.img %>" alt="<%= item.name %>">
                    <h1><%= item.id%></h1>
                </div>
            <% }); %> 
        </div>
    </div>
    <%- include('partials/search-script') %>
</body>
</html>
